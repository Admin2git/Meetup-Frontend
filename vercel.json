{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/events/(.*)",
            "dest": "/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}